{
  "Function": {
    "prefix": "func",
    "body": [
      "func ${1:name}(${2:args}) -> ${3:int} {",
      "  ${0}",
      "}"
    ],
    "description": "Define uma função"
  },
  "Main function": {
    "prefix": "main",
    "body": [
      "func main() -> int {",
      "  ${0}",
      "  return 0;",
      "}"
    ],
    "description": "Função principal padrão"
  },
  "For loop": {
    "prefix": "for",
    "body": [
      "for (${1:i: int} = ${2:0}; ${1} < ${3:10}; ${1} = ${1} + ${4:1}) {",
      "  ${0}",
      "}"
    ],
    "description": "Estrutura de repetição for"
  },
  "If else": {
    "prefix": "if",
    "body": [
      "if (${1:cond}) {",
      "  ${0}",
      "} else {",
      "  ",
      "}"
    ],
    "description": "Estrutura if/else"
  },
  "Print": {
    "prefix": "print",
    "body": [
      "print(${1:expr});"
    ],
    "description": "Função print"
  }
}
